{
  "name": "clothing-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky install",
    "migrate:up": "cross-env NODE_ENV=development node scripts/migrate.js up",
    "migrate:up:force": "cross-env NODE_ENV=development node scripts/migrate.js up:force",
    "migrate:down": "cross-env NODE_ENV=development node scripts/migrate.js down",
    "migrate:status": "cross-env NODE_ENV=development node scripts/migrate.js status",
    "migrate:up:prod": "cross-env NODE_ENV=production node scripts/migrate.js up",
    "migrate:up:force:prod": "cross-env NODE_ENV=production node scripts/migrate.js up:force",
    "migrate:down:prod": "cross-env NODE_ENV=production node scripts/migrate.js down",
    "migrate:status:prod": "cross-env NODE_ENV=production node scripts/migrate.js status"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "pg": "^8.11.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "typescript-eslint": "^8.44.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
